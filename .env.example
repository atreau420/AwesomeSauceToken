# --- WebSocket ---
WS_PORT=0

# --- Marketplace ---
MARKETPLACE_TREASURY=0xYOUR_TREASURY_WALLET_ADDRESS

# --- Stripe / Payments (if needed) ---
STRIPE_SECRET_KEY=
STRIPE_WEBHOOK_SECRET=
